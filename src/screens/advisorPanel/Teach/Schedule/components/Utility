import {routes} from '../../../../../API/APIRoutes';
import {generalRequest} from '../../../../../API/Utility';

export const fetchMySchedules = (token, filter = '') => {
  return generalRequest(
    filter === undefined
      ? routes.getTeachSchedules
      : routes.getTeachSchedules + '?' + filter,
    'get',
    undefined,
    'data',
    token,
  );
};
